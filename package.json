{
  "name": "f1-pwa-monorepo",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:migrate": "pnpm --filter @f1/api prisma migrate dev",
    "db:seed": "pnpm --filter @f1/api prisma db seed"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  }
}
